{
    "name": "smoothScroll",
    "displayName": "smoothScroll",
    "description": "Scroll smoothly with pgup, pgdwn",
    "version": "0.0.1",
    "publisher": "cyansprite",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.scrollDown",
        "onCommand:extension.scrollUp"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Smooth Scroll Properties",
            "properties": {
                "smoothscroll.linesToScroll": {
                    "type": "number",
                    "default": 15,
                    "description": "How many lines would you like to scroll?"
                },
                "smoothscroll.totalDelay": {
                    "type": "number",
                    "default": 90,
                    "description": "How long do you want it to be smooth (ms)"
                }
            }
        },
        "keybindings": [
            {
                "key": "pagedown",
                "command": "extension.scrollDown",
                "when": "editorTextFocus"
            },
            {
                "key": "pageup",
                "command": "extension.scrollUp",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}