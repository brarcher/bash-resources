set colored-stats
"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[7~": beginning-of-line
"\e[8~": end-of-line
"\eOH": beginning-of-line
"\eOF": end-of-line
"\e[H": beginning-of-line
"\e[F": end-of-line
"\C-k": history-search-backward
"\C-j": history-search-forward
"\C-v": "$(fvim)"
"\er": redraw-current-line
"\C-g\C-f": "$(gf)\e\C-e\er"
"\C-g\C-b": "$(gb)\e\C-e\er"
"\C-g\C-t": "$(gt)\e\C-e\er"
"\C-g\C-h": "$(gh)\e\C-e\er"
"\C-g\C-r": "$(gr)\e\C-e\er"
